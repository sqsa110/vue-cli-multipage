$header_height = 25px
$header_img_width = 25px
$header_padding_top = 5px
$header_margin = 10px
$header_background_color = #00ee00
$font_color = #fff

#fd_header
  position relative
  overflow auto
  background $header_background_color
  height $header_height
  padding $header_padding_top 0

  .icon
    font-size 2.5rem
    color $font_color

.goback,.gohome
  position absolute
  top $header_padding_top

.goback
  left $header_margin

.gohome
  right $header_margin

.header_title
  font-size 1.6rem
  height 100%
  line-height $header_height
  width 100%
  text-align center
  color $font_color


